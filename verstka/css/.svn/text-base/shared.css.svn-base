.img_big_menu_header	{padding:7px 10px 10px 20px; border-bottom:1px solid #d0d0d0; margin:0 0 5px 0; font-weight:bold;}
.img_big_menu_link	{padding:0 0 8px 0;}
	.img_big_menu_link li	{list-style:none; text-transform:uppercase;}
		.img_big_menu_link a	{display:block; text-decoration:none; padding:6px 10px 7px 20px; color:#191211;}
		.img_big_menu_link a:hover	{background:#ccebd6;}
		.img_big_menu_link a.active	{background:#ccebd6;}
.img_big_menu_min_img	{padding:0 0 0 10px;}
