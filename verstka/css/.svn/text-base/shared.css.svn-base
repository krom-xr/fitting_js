		.img_big_menu_min_img img	{width:53px; padding:1px; border:1px solid #d0d0d0;}
		.img_big_menu_min_img a:hover img	{border:1px solid #009933;}
		.img_big_menu_min_img a.active img	{border:1px solid #009933;}
		
