	.edit_form td a:hover{border-bottom:none;}
	.edit_form td a.save{font-size:18px;border-bottom:none; color:#ecedef; background-color:#009933;font-weight:bold;font-weight:normal;padding:3px 18px; text-transform:uppercase;text-decoration:none;margin:0px 21px 0px 0px;}
	.edit_form td a.save:hover{border-bottom:none;text-decoration:underline;}
	.edit_form td input {font-size:12px; color:#94908f; font-weight:normal; border:1px solid #cecece; line-height:28px;height:28px; width:258px; padding:0px 10px;}
