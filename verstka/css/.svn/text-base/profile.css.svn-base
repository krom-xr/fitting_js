	.edit_form td select.month {width:82px;}
	.edit_form td.big_select select { width:280px;}
	.edit_form td option {font-size:12px; color:#94908f; font-weight:normal; line-height:28px; height:28px; padding:0px 0px 0px 10px;}
	.edit_form td textarea {font-size:12px; color:#94908f; font-weight:normal; border:1px solid #cecece; height:98px;  width:263px; padding:5px 5px 5px 10px;}
	
