.product_list	{ position:relative;width:417px;height:621px; margin:0; overflow: hidden;}
.carousel,
.product_list .carousel-wrap{margin:0;border:0;zoom:1; overflow: hidden;}
.product_list .carousel-wrap{display:inline-block;vertical-align:middle;width:417px;height:582px; z-index:10; margin:0;}
.IE .js .carousel-wrap{display:inline;}
.product_list ul{margin:0;padding:0;zoom:1;}
.product_list ul li{overflow:hidden;display:inline;float:left;width:417px;height:582px;text-align:center;}
.product_list .carousel-control{overflow:hidden;display:inline-block; z-index:111; position:absolute; bottom:9px; vertical-align:middle;width:6px;height:10px;cursor:pointer;line-height:999px;zoom:1;}
.product_list .carousel-next{background:url( '../images/next1.png' ) 0 0 no-repeat;}
.product_list .carousel-previous{background:url( '../images/prev1.png' ) 0 0 no-repeat;}
.product_list .carousel-control:hover{-moz-opacity:.7;opacity:0.70;filter:alpha(opacity=70);}
.product_list .disabled,.product_list .disabled:hover{-moz-opacity:.1;opacity:0.10;filter:alpha(opacity=10);cursor:default;}
.product_list ul{padding:0;list-style:none;}
.product_list ul li{position:relative;width:417px; height:582px;margin:0; overflow:hidden;}

.product_list div.center-wrap{overflow: visible;clear:both;zoom:1; position: absolute; width:417px; left:0; bottom:9px; z-index:100; height:11px;}
.product_list div.center-wrap div.carousel-pagination{border:0;margin:0px 0; position:absolute; top:0; left:50%;}
.product_list div.center-wrap div.carousel-pagination p{ padding:0 0 0 0; height:11px; text-align:center;}
.product_list div.center-wrap a{ width:11px; height:11px; display:inline-block; margin:0px 3px; background:url( '../images/dots1.png' ) 0 0 no-repeat; cursor:pointer;}
.product_list div.center-wrap a.hover,
.product_list div.center-wrap a.active{background-position:100% 0;}
