.j_carusel	{ width:992px; overflow:hidden; border:3px solid #e7e7e7; margin:0 auto 29px auto;}
.jcarousel-skin-tango .jcarousel-container {}
.jcarousel-skin-tango .jcarousel-direction-rtl {direction: rtl;}
.jcarousel-skin-tango .jcarousel-container-horizontal { width: 949px; padding: 13px 21px;}

.jcarousel-skin-tango .jcarousel-clip {overflow: hidden;}
.jcarousel-skin-tango .jcarousel-clip-horizontal {width:  949px; height: 162px;}
.jcarousel-skin-tango .jcarousel-item { width: 112px; height: 162px;}

.jcarousel-skin-tango .jcarousel-item-horizontal 		{ margin-left: 0; margin-right: 8px;}
.jcarousel-skin-tango .jcarousel-item-horizontal div	{ font-size:12px; padding-top:3px; font-weight:bold; color:#00733a;}
.jcarousel-skin-tango .jcarousel-item-horizontal img	{ border:1px solid #fff;}
.jcarousel-skin-tango .jcarousel-item-horizontal a:hover img	{ border:1px solid #b2afae;opacity: 0.80;filter:alpha(opacity=80);}
.jcarousel-skin-tango .jcarousel-item-horizontal a		{ display:block;}

.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-item-horizontal {	margin-left: 10px; margin-right: 0;}

.jcarousel-skin-tango .jcarousel-item-placeholder { background: #fff;color: #000;}

/**
 *  Horizontal Buttons
 */
.jcarousel-skin-tango .jcarousel-next-horizontal {
    position: absolute;
    top: 14px;
    right: 0px;
    width: 21px;
    height: 144px;
    cursor: pointer;
    background: transparent url( '../images/next.gif' ) no-repeat 50% 50%;
}

.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-next-horizontal {
    left: 5px;
    right: auto;
    background-image: url( '../images/next.gif' );
}

.jcarousel-skin-tango .jcarousel-next-horizontal:hover,
.jcarousel-skin-tango .jcarousel-next-horizontal:focus {
    background-position: 50% 50%;
}

.jcarousel-skin-tango .jcarousel-next-horizontal:active {
    background-position: 50% 50%;
}

.jcarousel-skin-tango .jcarousel-next-disabled-horizontal,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:hover,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:focus,
.jcarousel-skin-tango .jcarousel-next-disabled-horizontal:active {
    cursor: default;
    background-position: 50% 50%;
}

.jcarousel-skin-tango .jcarousel-prev-horizontal {
    position: absolute;
    top: 14px;
    left: 0px;
    width: 21px;
    height: 144px;
    cursor: pointer;
    background: transparent url( '../images/prev.gif' ) no-repeat 50% 50%;
}

.jcarousel-skin-tango .jcarousel-direction-rtl .jcarousel-prev-horizontal {
    left: auto;
    right: 5px;
    background-image: url( '../images/prev.gif' );
}

.jcarousel-skin-tango .jcarousel-prev-horizontal:hover, 
.jcarousel-skin-tango .jcarousel-prev-horizontal:focus {
    background-position: 50% 50%;
}

.jcarousel-skin-tango .jcarousel-prev-horizontal:active {
    background-position: 50% 50%;
}

.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:hover,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:focus,
.jcarousel-skin-tango .jcarousel-prev-disabled-horizontal:active {
    cursor: default;
    background-position: 50% 50%;
}

.vk_content .j_carusel	{ width:772px; overflow:hidden; border:3px solid #e7e7e7; margin:0 auto 0px auto;}
.vk_content .jcarousel-skin-tango .jcarousel-container-horizontal { width: 730px;}
.vk_content .jcarousel-skin-tango .jcarousel-clip-horizontal {width:  730px;}
.vk_content .jcarousel-skin-tango .jcarousel-item-horizontal 		{ margin-left: 0; margin-right:11px;}

