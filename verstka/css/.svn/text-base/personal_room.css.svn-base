.lk_menu	{ width:200px; overflow:hidden;}
.lk_menu .lk_img	{ margin-bottom:20px;}
.lk_menu .lk_img img	{ border:1px solid #9b9ea0; padding:1px; margin-bottom:10px;}
.lk_menu .lk_img a	{ display:inline-block; font-weight:bold; color:#191211; text-decoration:none;}
.lk_menu ul		{ overflow:hidden; list-style:none; margin-bottom:20px;}
.lk_menu ul li		{ overflow:hidden; list-style:none;}
.lk_menu ul li span	{ display:block; color:#161719; font:bold 18px Candara, Calibri; background:#ececec; line-height:30px; padding-left:13px; text-transform:uppercase;}
.lk_menu ul li a	{ display:block; color:#191211; font-size:14px; line-height:29px; padding-left:13px; border-bottom:1px solid #d2d1d0; text-decoration:none;}
.lk_menu ul li a:hover	{ text-decoration:underline;}
.lk_menu ul li.active a		{ text-decoration:none; color:#00733A;}
